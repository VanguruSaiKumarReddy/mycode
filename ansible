- name: "5.5.1.h | Ensure pwquality ucredit is set to -1"
  lineinfile:
    path: /etc/security/pwquality.conf
    regexp: '^\s*ucredit\s*='
    line: 'ucredit = -1'
    create: yes
    mode: '0644'
    backup: yes
  tags: ['cis_5_5_1_h','cis','pwquality','password']


grep -E '^\s*ucredit\s*=' /etc/security/pwquality.conf
