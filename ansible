- name: "5.6.3.c | Ensure TMOUT is configured via /etc/profile.d/tmout.sh"
  copy:
    dest: /etc/profile.d/tmout.sh
    content: |
      TMOUT=900
      readonly TMOUT
      export TMOUT
    owner: root
    group: root
    mode: '0644'
  tags: ['cis_5_6_3_c','cis','tmout']


grep TMOUT /etc/profile.d/tmout.sh
